		{
			"name": "Get single user(Practice pre-post script)",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"console.log(\"This is simple prerequest\");\r",
							"pm.environment.set(\"randomVal\", Math.floor(Math.random() * 12) + 1);\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {   //Basic Status Code Validation",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Response contains expected text\", function () {",
							"    pm.expect(pm.response.text()).to.include(\"success\");",
							"});",
							"",
							"",
							"pm.test(\"Content-Type is present\", function () {  //Validate Content-Type ",
							"    pm.response.to.have.header(\"Content-Type\"); ",
							"",
							"});",
							" ",
							"pm.test(\"Content-Type is application/json\", function () { //Validate Content-Type Header",
							"pm.response.to.have.header(\"Content-Type\");   ",
							"pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\"); ",
							"});",
							"",
							"pm.test(\"Response time is less than 500 ms\", function () { //Validate Response Time",
							"    pm.expect(pm.response.responseTime).to.be.below(500);",
							"});",
							"",
							"pm.test(\"To check id \", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.data.id).is.below(15);",
							"});",
							"",
							"",
							"let response = pm.response.json();   // Parse the response body as JSON",
							"",
							"pm.test(\"Validate basic schema\", function () {",
							"    pm.expect(response.data).to.have.all.keys(\"id\",\"name\",\"year\",\"color\",\"pantone_value\");",
							"});",
							"",
							"",
							"// 1. Check top-level keys",
							"pm.test(\"Check main keys exist\", function () {",
							"    pm.expect(response).to.have.property(\"data\");",
							"    pm.expect(response).to.have.property(\"support\");",
							"    pm.expect(response).to.have.property(\"_meta\");",
							"});",
							"",
							"// 2. Check fields inside \"data\"",
							"pm.test(\"Validate data fields\", function () {",
							"    pm.expect(response.data).to.have.property(\"id\");",
							"});",
							"// check string",
							"pm.test(\"Body matches string\", function () {  ",
							"    pm.expect(pm.response.text()).to.include(\"mimosa\");",
							"});",
							"    ",
							" pm.test(\"Validate support data\", function () {    ",
							"pm.expect(response.data).to.have.property(\"email\");",
							"pm.expect(response.data).to.have.property(\"first_name\");",
							"pm.expect(response.data).to.have.property(\"last_name\");",
							" pm.expect(response.data).to.have.property(\"avatar\");",
							"});",
							"",
							"//  Check fields inside \"support\"",
							"pm.test(\"Validate support fields\", function () {",
							"    pm.expect(response.support).to.have.property(\"url\");",
							"    pm.expect(response.support).to.have.property(\"text\");",
							"});",
							"",
							"//  Check fields inside \"_meta\"",
							"pm.test(\"Validate _meta fields\", function () {",
							"    pm.expect(response._meta).to.have.property(\"powered_by\");",
							"    pm.expect(response._meta).to.have.property(\"upgrade_url\");",
							"    pm.expect(response._meta).to.have.property(\"docs_url\");",
							"    pm.expect(response._meta).to.have.property(\"template_gallery\");",
							"    pm.expect(response._meta).to.have.property(\"message\");",
							"    pm.expect(response._meta).to.have.property(\"features\");",
							"    pm.expect(response._meta).to.have.property(\"upgrade_cta\");",
							"});",
							"",
							"",
							"pm.test(\"Fields should not be null\", function () {",
							"    pm.expect(response.data.first_name).to.not.be.null;",
							"    pm.expect(response.data.last_name).to.not.be.null;",
							"});",
							"",
							"pm.test(\"Check cookies\", function () { // ",
							"    pm.expect(pm.cookies.has(\"session_id\")).to.be.true;",
							"});",
							"  ",
							"//Simple success Code Validations",
							"pm.test(\"Should return 200\", () => {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"// Simple method validation Validations",
							"pm.test(\"Method is GET\", function () {",
							"    pm.expect(pm.request.method).to.eql(\"GET\");",
							"});",
							"",
							"  "
						],
